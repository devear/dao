ceRoot":"src/views","sourcesContent":["<template>\r\n  <div>\r\n    <svg>\r\n\r\n    </svg>\r\n        <circle v-for=\"(node, index) in nodePositions\"\r\n        :key=\"index\"\r\n        :cx=\"node.x\"\r\n        :cy=\"node.y\"\r\n        :fill=\"fillColor(node.value)\"\r\n        :r=\"node.radius\"></circle>\r\n    <v-row>\r\n        <v-col\r\n        cols=\"12\"\r\n        md=\"12\"\r\n        class=\"text-center\"\r\n        >\r\n            <v-tooltip bottom>\r\n                <template v-slot:activator=\"{ on, attrs }\">\r\n                    <span\r\n                    v-bind=\"attrs\"\r\n                    v-on=\"on\"\r\n                    class=\"text-body-2\"\r\n                    >{{userName}}<br>Баланс: {{balance}}</span>\r\n                </template>\r\n                <span>{{address}}</span>\r\n            </v-tooltip>\r\n        </v-col>\r\n    </v-row>\r\n    <v-row>\r\n        <v-col\r\n        cols=\"12\"\r\n        md=\"4\"\r\n        class=\"text-center\"\r\n        >\r\n            <v-tooltip bottom>\r\n                <template v-slot:activator=\"{ on, attrs }\">\r\n                    <span\r\n                    v-bind=\"attrs\"\r\n                    v-on=\"on\"\r\n                    class=\"text-body-2\"\r\n                    >{{addresses[0].name}}<br>Баланс: {{addresses[0].balance}}</span>\r\n                </template>\r\n                <span>{{addresses[0].address}}</span>\r\n            </v-tooltip>\r\n        </v-col>\r\n        <v-col\r\n        cols=\"12\"\r\n        md=\"4\"\r\n        class=\"text-center\"\r\n        style=\"border-left: 1px solid #ccc; border-right: 1px solid #ccc\"\r\n        >\r\n            <v-tooltip bottom>\r\n                <template v-slot:activator=\"{ on, attrs }\">\r\n                    <span\r\n                    v-bind=\"attrs\"\r\n                    v-on=\"on\"\r\n                    class=\"text-body-2\"\r\n                    >{{addresses[1].name}}<br>Баланс: {{addresses[1].balance}}</span>\r\n                </template>\r\n                <span>{{addresses[1].address}}</span>\r\n            </v-tooltip>\r\n        </v-col>\r\n        <v-col\r\n        cols=\"12\"\r\n        md=\"4\"\r\n        class=\"text-center\"\r\n        >\r\n            <v-tooltip bottom>\r\n                <template v-slot:activator=\"{ on, attrs }\">\r\n                    <span\r\n                    v-bind=\"attrs\"\r\n                    v-on=\"on\"\r\n                    class=\"text-body-2\"\r\n                    >{{addresses[2].name}}<br>Баланс: {{addresses[2].balance}}</span>\r\n                </template>\r\n                <span>{{addresses[2].address}}</span>\r\n            </v-tooltip>\r\n        </v-col>\r\n    </v-row>\r\n    <v-row>\r\n        <v-col\r\n        cols=\"12\"\r\n        md=\"2\"\r\n        class=\"text-center\"\r\n        >\r\n            <v-tooltip bottom>\r\n                <template v-slot:activator=\"{ on, attrs }\">\r\n                    <span\r\n                    v-bind=\"attrs\"\r\n                    v-on=\"on\"\r\n                    class=\"text-body-2\"\r\n                    >{{addresses[3].name}}<br>Баланс: {{addresses[3].balance}}</span>\r\n                </template>\r\n                <span>{{addresses[3].address}}</span>\r\n            </v-tooltip>\r\n        </v-col>\r\n        <v-col\r\n        cols=\"12\"\r\n        md=\"2\"\r\n        class=\"text-center\"\r\n        >\r\n            <v-tooltip bottom>\r\n                <template v-slot:activator=\"{ on, attrs }\">\r\n                    <span\r\n                    v-bind=\"attrs\"\r\n                    v-on=\"on\"\r\n                    class=\"text-body-2\"\r\n                    >{{addresses[4].name}}<br>Баланс: {{addresses[4].balance}}</span>\r\n                </template>\r\n                <span>{{addresses[4].address}}</span>\r\n            </v-tooltip>\r\n        </v-col>\r\n        <v-col\r\n        cols=\"12\"\r\n        md=\"2\"\r\n        class=\"text-center\"\r\n        style=\"border-left: 1px solid #ccc\"\r\n        >\r\n            <v-tooltip bottom>\r\n                <template v-slot:activator=\"{ on, attrs }\">\r\n                    <span\r\n                    v-bind=\"attrs\"\r\n                    v-on=\"on\"\r\n                    class=\"text-body-2\"\r\n                    >{{addresses[5].name}}<br>Баланс: {{addresses[5].balance}}</span>\r\n                </template>\r\n                <span>{{addresses[5].address}}</span>\r\n            </v-tooltip>\r\n        </v-col>\r\n        <v-col\r\n        cols=\"12\"\r\n        md=\"2\"\r\n        class=\"text-center\"\r\n        style=\"border-right: 1px solid #ccc\"\r\n        >\r\n            <v-tooltip bottom>\r\n                <template v-slot:activator=\"{ on, attrs }\">\r\n                    <span\r\n                    v-bind=\"attrs\"\r\n                    v-on=\"on\"\r\n                    class=\"text-body-2\"\r\n                    >{{addresses[6].name}}<br>Баланс: {{addresses[6].balance}}</span>\r\n                </template>\r\n                <span>{{addresses[6].address}}</span>\r\n            </v-tooltip>\r\n        </v-col>\r\n        <v-col\r\n        cols=\"12\"\r\n        md=\"2\"\r\n        class=\"text-center\"\r\n        >\r\n            <v-tooltip bottom>\r\n                <template v-slot:activator=\"{ on, attrs }\">\r\n                    <span\r\n                    v-bind=\"attrs\"\r\n                    v-on=\"on\"\r\n                    class=\"text-body-2\"\r\n                    >{{addresses[7].name}}<br>Баланс: {{addresses[7].balance}}</span>\r\n                </template>\r\n                <span>{{addresses[7].address}}</span>\r\n            </v-tooltip>\r\n        </v-col>\r\n        <v-col\r\n        cols=\"12\"\r\n        md=\"2\"\r\n        class=\"text-center\"\r\n        >\r\n            <v-tooltip bottom>\r\n                <template v-slot:activator=\"{ on, attrs }\">\r\n                    <span\r\n                    v-bind=\"attrs\"\r\n                    v-on=\"on\"\r\n                    class=\"text-body-2\"\r\n                    >{{addresses[8].name}}<br>Баланс: {{addresses[8].balance}}</span>\r\n                </template>\r\n                <span>{{addresses[8].address}}</span>\r\n            </v-tooltip>\r\n        </v-col>\r\n    </v-row>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  import TronWeb from 'tronweb'\r\n  import * as d3 from \"d3\"\r\n  import chroma from 'chroma-js'\r\n  //import TronGrid from 'trongrid'\r\n    window.d3 = d3\r\n    const fillColor = d3\r\n    .scaleQuantile()\r\n    .domain(['low', 'medium', 'high'])\r\n    .range(chroma.scale(chroma.brewer.RdYlBu.reverse()).colors(3))\r\n\r\n  export default {\r\n    data: () => ({\r\n        fillColor,\r\n      center:        { x: 0, y: 0 },\r\n      forceStrength: 0.03,\r\n      rawData:       null,\r\n      /** @type {d3.Simulation} */\r\n      simulation:  null,\r\n      yearCenters: {\r\n        2008: {\r\n          x: -200,\r\n          y: 0\r\n        },\r\n        2009: {\r\n          x: 0,\r\n          y: 0\r\n        },\r\n        2010: {\r\n          x: 200,\r\n          y: 0\r\n        },\r\n        low: {\r\n          x: -500,\r\n          y: 333\r\n        },\r\n        medium: {\r\n          x: 0,\r\n          y: 0\r\n        },\r\n        high: {\r\n          x: 300,\r\n          y: -500\r\n        }\r\n      },\r\n      showYears:     false,\r\n      orgNames:      [],\r\n      nodePositions: [],\r\n\r\n      address: 'TPszgQEkvbtduZyDCC6ukKyr279pefsNFo',\r\n      fullNode: \"https://api.shasta.trongrid.io\",\r\n      solidityNode: \"https://api.shasta.trongrid.io\",\r\n      eventServer: \"https://api.shasta.trongrid.io\",\r\n      privateKey: '795b0060d28320d0348db4d9abfebe6629034de03c11a15cec8695cf3646302a',\r\n      //trc20ContractAddress: \"TF94ubDQo2cP78Utx4eEotp4iyBeBwNwpB\", //\"TR7NHqjeKQxGTCi8q8ZY4pL8otSzgjLj6t\",\r\n      tr: true,\r\n      balance: 0,\r\n      decimals: 1000000,\r\n      addresses: [\r\n          {name: 'user2', address: 'TFPMa6SnKmSZf68toufZ8vwF3n4fJbeFcH', balance: 0},\r\n          {name: 'user3', address: 'TQ7uGAiGbXPHbxag2nQMANqvgvtQ1JHbpM', balance: 0},\r\n          {name: 'user4', address: 'TV49PVVxXDMwbcmv69rojRHLrwWML7LuXu', balance: 0},\r\n          {name: 'user5', address: 'TJfVuYfKMkwgU9KTZghxdy8v6s83qiYUDq', balance: 0},\r\n          {name: 'user6', address: 'TJj6AhdeoGgWytVBE3Mfp5mJVaSrfSHQ4R', balance: 0},\r\n          {name: 'user7', address: 'TFWWE3D3mZjwofLk5j6Vfp19Fj3p8UxDrS', balance: 0},\r\n          {name: 'user8', address: 'TRnBF1NhsRshzvr3zcKBmeehHGF9Mtt8Rc', balance: 0},\r\n          {name: 'user9', address: 'TK6v1P8rwTHPdmKC96q5qDnwyGU59gL86V', balance: 0},\r\n          {name: 'user10', address: 'TPLL9Lud7WZeL7EsbL6btk2HPBXULFQywL', balance: 0}\r\n      ]\r\n    }),\r\n    computed: {\r\n        userName(){\r\n            const name = this.$store.getters.info ? '' + this.$store.getters.info.name + '' : 'Гость'\r\n            return name\r\n        },\r\n        nodes() {\r\n            if (!this.rawData) return\r\n            // Use the max total_amount in the data as the max in the scale's domain\r\n            // note we have to ensure the total_amount is a number.\r\n            const maxAmount = d3.max(this.rawData, d => {\r\n                return +d.total_amount\r\n            })\r\n            const domainVal = [0, maxAmount]\r\n            fillColor.domain(domainVal)\r\n            // Sizes bubbles based on area.\r\n            // @v4: new flattened scale names.\r\n            const radiusScale = d3\r\n                .scalePow()\r\n                .exponent(0.6)\r\n                .range([5, 85])\r\n                .domain(domainVal)\r\n            // Use map() to convert raw data into node data.\r\n            // Checkout http://learnjsdata.com/ for more on\r\n            // working with data.\r\n            this.orgNames = []\r\n            const myNodes = this.rawData.map(d => {\r\n                if (!this.orgNames.includes(d.group)) {\r\n                this.orgNames.push(d.group)\r\n                }\r\n                return {\r\n                id:     d.id,\r\n                radius: radiusScale(+d.total_amount),\r\n                value:  +d.total_amount,\r\n                name:   d.grant_title,\r\n                org:    d.organization,\r\n                group:  d.group,\r\n                year:   d.start_year,\r\n                x:      Math.random() * 900,\r\n                y:      Math.random() * 800\r\n                }\r\n            })\r\n            // sort them to prevent occlusion of smaller nodes.\r\n            myNodes.sort((a, b) => {\r\n                return b.value - a.value\r\n            })\r\n            return myNodes\r\n        }\r\n    },\r\n    methods: {\r\n        charge(d) {\r\n        return -this.forceStrength * Math.pow(d.radius, 2)\r\n        },\r\n        ticked() {\r\n            this.nodePositions = Object.freeze(\r\n                this.simulation.nodes().map(v => ({\r\n                x:      v.x,\r\n                y:      v.y,\r\n                radius: v.radius,\r\n                value:  v.value\r\n                }))\r\n            )\r\n        }\r\n    },\r\n    async mounted(){\r\n      const tronWeb = new TronWeb(this.fullNode, this.solidityNode, this.eventServer, this.privateKey)\r\n      //const tronGrid = new TronGrid(tronWeb)\r\n      const contract = await tronWeb.contract().at(\"TYjsaL4BisZ3VMKGMti4ySvhBUYAyC14Tc\")\r\n\r\n      //console.log(await tronGrid.account.get(this.fonds[3].addr, {}))\r\n\r\n      while(this.tr){\r\n          this.balance = await contract.balanceOf(this.address).call() / this.decimals\r\n          for (let i = 0; i < this.addresses.length; i++) {\r\n            this.addresses[i].balance = await contract.balanceOf(this.addresses[i].address).call() / this.decimals\r\n          }\r\n      }\r\n\r\n      // d3.json('/static/demo_data/quantifiable/populations.json').then(res => {\r\n        d3.csv('/static/demo_data/gates.csv').then(res => {\r\n        this.rawData = res\r\n        })\r\n    },\r\n    watch: {\r\n        nodes() {\r\n        this.simulation = d3\r\n            .forceSimulation(this.nodes)\r\n            .velocityDecay(0.3)\r\n            .force(\r\n            'x',\r\n            d3\r\n                .forceX()\r\n                .strength(this.forceStrength)\r\n                .x(this.center.x)\r\n            )\r\n            .force(\r\n            'y',\r\n            d3\r\n                .forceY()\r\n                .strength(this.forceStrength)\r\n                .y(this.center.y)\r\n            )\r\n            .force('center', d3.forceManyBody().strength(this.charge))\r\n            .on('tick', this.ticked)\r\n        this.$nextTick(() => {\r\n            d3.drag(this.nodePositions)(d3.selectAll('circle'))\r\n        })\r\n        },\r\n        showYears(val) {\r\n        const r = Math.random() > 0.5\r\n        if (this.simulation) {\r\n            this.simulation.force(\r\n            'x',\r\n            d3\r\n                .forceX()\r\n                .strength(this.forceStrength)\r\n                .x(d => {\r\n                return val\r\n                    ? this.yearCenters[r ? d.group : d.year].x\r\n                    : this.center.x\r\n                })\r\n            )\r\n            this.simulation.force(\r\n            'y',\r\n            d3\r\n                .forceY()\r\n                .strength(this.forceStrength)\r\n                .y(d => {\r\n                return val\r\n                    ? this.yearCenters[r ? d.group : d.year].y\r\n                    : this.center.y\r\n                })\r\n            )\r\n            this.simulation.alpha(1.5).restart()\r\n        }\r\n        }\r\n    }\r\n  }\r\n</script>"]}]}