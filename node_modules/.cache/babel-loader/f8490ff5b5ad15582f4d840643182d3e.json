ta = {\r\n                email: this.email,\r\n                password: this.password,\r\n                name: this.name,\r\n                address: this.address,\r\n                pk: this.pk,\r\n                ref: this.ref\r\n            }\r\n            \r\n            try{\r\n                await this.$store.dispatch('signUp', formData)\r\n                this.$router.push('/')\r\n            }catch(e){\r\n                console.log(e)\r\n            }\r\n        }\r\n    }\r\n}\r\n</script>"],"sourceRoot":"src/views"}]}